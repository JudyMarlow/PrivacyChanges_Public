    <script type="text/javascript">
        var tc_vars = new Array();
		
		tc_vars["env_work"] = "PRODUCTION"; 
		tc_vars["env_dnt"] = "disabled"; 
		tc_vars["env_template"] = "ErrorSplashPage"; 

        var tagCmdScrptJS = '//cdn.tagcommander.com/1485/tc_1.js';
        var tagCmdScrpt = document.createElement( 'script' );
        tagCmdScrpt.src = tagCmdScrptJS;
        tagCmdScrpt.async = true;
        tagCmdScrpt.type = 'text/javascript';
        tagCmdScrpt.charset = 'utf-8';
        (document.getElementsByTagName( 'body' )[0] || document.getElementsByTagName( 'script' )[0].parentNode ).insertBefore( tagCmdScrpt, null );
	
    </script>
    <!-- IAdvize script. DO NOT ALTER ANYTHING-->
    <script language="javascript" type="text/javascript">
        var ruleTriggered = false;
        var speed = 600;
        var isChatOffline = false;

        iAdvizeCallbacks = {

            onRuleStarted: function ( obj ) {
                ruleTriggered = true;
            },
            onStatusChanged: function ( obj ) { // callback chat
                if ( ruleTriggered == false ) { return; }
                if ( obj.offline == 0 && obj.busy == 0 ) {
                    $( "div#button_offline_LRFR.idz_widget_off" ).hide();
                    $( ".call_chat" ).stop( true, true ).fadeIn( speed );
                } else {
                    $( "div#button_offline_LRFR.idz_widget" ).stop( true, true ).fadeIn( speed );
                    isChatOffline = true;
                    $(".call_chat").hide();
                }
            },
            onCallStatusChanged: function ( obj ) { // callback call

                if ( ruleTriggered == false ) { return; }
                if ( obj.offline == 0 ) {
                    $( "div#button_offline_LRFR.idz_widget_off" ).hide();
                    $( ".call_chat" ).stop( true, true ).fadeIn( speed );
                } else if ( obj.offline == 1 && isChatOffline ) {
                    $("div#button_offline_LRFR.idz_widget").stop(true, true).fadeIn(speed);
                    $(".call_chat").hide();
                }
            }
        };

    </script>
    <!-- End of IAdvize script.-->
	
