	/**
	 * Patch for TE-5248
	 */
	page_params.runHeadJs = false;
	/**
	 * TE-5398
	 */
	// the following three settings should not change from the current implementation
	var timing_appId = 16,
		timing_productId = 2,
		timing_pageType = 'home',
		mgPerformanceCallbacks = [
			function(){ // or if you don't
				// do something
								//Load files via Head JS
				var siteHead = new Load_scripts();
				siteHead.init({
					'jsFileList'	: jsFileList, //json object with file list
					'head'			: head //head.js plugin object
				});
				page_params.runHeadJs = true;
							}
		];
		if( !page_params.runHeadJs ){
		setTimeout(function(){
			var siteHead = new Load_scripts();
			siteHead.init({
				'jsFileList'	: jsFileList, //json object with file list
