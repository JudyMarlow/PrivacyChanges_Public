			page_params.autocomplete_rows_setUp = {
				'queries' : 0,
				'pornstars' : 0,
				'channels' : 0
			};
		</script>
		<script type="text/javascript">
	var autocomplete_url = "http://" + location.hostname + "/",
		urlSegment = function(){
			var mySegmentUrl = "";

			if(page_params.segment === 1){
				mySegmentUrl = "gay/";
			} else if( page_params.segment === 2){
				mySegmentUrl = "shemale/";
			}

			return mySegmentUrl;
		};
	/**
	 * AUTO COMPLETE SET UP GENERAL PLATFORM VALUES.
	 * ***/
	page_params.autocomplete_setup = {
		/**
		 * URL AJAX SET UP
		 */
		'ajaxUrl'		: autocomplete_url + 'ajax2/searchAutoComplete/?c=US&segment=' + page_params.segment,
		'isDesktop'		: page_params.myPlatform === "pc",
		'categories_setup' : {
			'queries' : {
				'url' : autocomplete_url +  urlSegment() + "searches.html?q=",
				'url_space_replace' : "+",
				'rows' : page_params.autocomplete_rows_setUp.queries
			}
//			'pornstars' : {
//				'url' : autocomplete_url + "pornstar",
//				'url_space_replace' : "+",
//				'rows' : page_params.autocomplete_rows_setUp.pornstars
//			},
//			'channels' : {
//				'url' : autocomplete_url + "channel",
//				'url_space_replace' : "+",
//				'rows' : page_params.autocomplete_rows_setUp.channels
//			}
		},
		'delta' : 500
	};
</script>

