<script type="text/javascript" language="javascript">
function chkSrch(fld){
  if (fld.value == 'Search by Keyword or Item #' || fld.value == '')
  {
      alert('Please enter keyword(s) or item number and try again.'); 
      fld.focus();
      return false;
  } else {
  return true;
  }
}
</script>
