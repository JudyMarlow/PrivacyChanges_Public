    function getUserInfo()
    {
        var JsonObject = new Object(); 
        JsonObject.name="";
        JsonObject.enrollmentDate="01/01/0001";
        JsonObject.membershipType="visitor";
        JsonObject.info_MemberNumber="";
        var jsonText = JSON.stringify(JsonObject);

        if(JSON.parse(jsonText))
        {
            return jsonText;
        }
    }

    
    function fLogin(isWWButtonClicked)
    {
	    javascript:void(loginPopup('/legal/privacy.aspx','false', isWWButtonClicked));
    }

    function TopNavFbLogin()
    {
		var isWWButtonClicked = false;
	    javascript:void(loginPopup('/legal/privacy.aspx','true', isWWButtonClicked));
    }

    function fLogout(setURL)
    {
	    logoutPopup(false, setURL);
	    
	    if(PlanManagerIsActive()) 
	    {
	        planManagerPopupWindow.close();
	    }
    }
	 

         $(document).ready(function ($) {
                    $('#ww_menu').dcMegaMenu({
rowItems:  '5',
speed:  'fast',
effect:  'fade'
                    });
                });
          $(function () {
                    /*Megamenus*/
                    $('#ww_menu').click(function () {/*set up a click event for the li*/
                        $(this).parents('.closex').click(); /*click any other lis which are active to close their menus*/
                        $(this).find('.closex').toggle(); /*toggle the child megamenu*/
                        $(this).toggleClass('hover'); /*toggle a class so we can identify any open megamenus*/
                    });

                    $('.closex').click(function (event) {/*hide the megamenu on load and set up a click event..*/
                        $(this).parents('li').click(); /*..that just clicks the parent li*/
                        event.stopPropagation(); /*stop the click bubbling up to the parent li*/
                    });
                });

</script>

