    <script>
    
        jQuery(window).scroll(function() {
            if (jQuery(this).scrollTop() > 1){  
                jQuery('.tm-toolbar').slideUp(150);
                jQuery('header').addClass('sticky');
            }
            else{
                jQuery('.tm-toolbar').slideDown(150);
                jQuery('header').removeClass('sticky');
            }
        });

        jQuery(document).ready(function($) {
          
          var rx = /\/\w*/;
          var langCode = ['/en_us'];

          if (window.location.pathname.length>1){
			langCode = rx.exec(window.location.pathname);
          } else {
          	if (window.location.host.indexOf('uk.com') > -1) {
          		console.log('uk');
          		langCode = ['/en_gb'];
          	}
          }
          $('a#login').attr('href',window.location.protocol+'//'+window.location.host+langCode[0]+'/login');
          $('a#newuser').attr('href',window.location.protocol+'//'+window.location.host+'/newuser_type.asp?lang='+langCode[0].substr(1));
          $('a#logo-link').attr('href', window.location.protocol+'//'+window.location.host+langCode[0]+'/');

          $('.bannergroup').bjqs({
            width         : 10000,
            height        : 500,
            animtype      : 'slide',
            responsive    : true, 
            automatic     : true
          });

          $('[data-lightbox]').magnificPopup({type:'iframe'});

            if (document.location.hash) {
                var myAnchor = document.location.hash;
                $(myAnchor).attr('tabindex', -1).on('blur focusout', function () {
                    $(this).removeAttr('tabindex');
                }).focus();
            }

            $('.skip-link a').click(function(){
                $('#main-content').attr('tabindex', -1).on('blur focusout', function () {
                    $(this).removeAttr('tabindex');
                }).focus();
            });

            $('.stripe.trigger-animation').each(function(){
                var position = $(this).offset().top;
                var target = $(this).find('svg');

                $(window).scroll(function(){

                    if ( $(this).scrollTop() >= position-400){
                        // console.log(target);
                        target.attr('class', 'animate');
                    }
                });
            });

                              
                    
        });

</script>
                                            
