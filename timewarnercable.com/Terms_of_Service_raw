    <script>
    var ieVersion= "";
    if(typeof getIEVersion === 'function'){
        ieVersion= getIEVersion();
    }
    //Prevent this from running, when included in CIC tiles and Browser is IE 8 and IE 11
    //IE handler for RTE in CIC tiles would be in expandable-tile.js
    if(!(jQuery('#b2f39fc8-91a8-447a-a30c-d9ffbdd6062f').parents('.expandable_tile').length  > 0 && (ieVersion >=8 && ieVersion <= 11))){
        jQuery(document).ready(function(){
            if (window.Twc && Twc.Tooltip && typeof Twc.Tooltip.init === 'function') {
                jQuery('#b2f39fc8-91a8-447a-a30c-d9ffbdd6062f').find('a.tooltip').each(function() { Twc.Tooltip.init($(this)) });
            }
            var $aLink = jQuery('#b2f39fc8-91a8-447a-a30c-d9ffbdd6062f').find('a[target="_blank"]');
            if($aLink && $aLink.length > 0 && typeof $aLink.newWindowLink === 'function') {
                $aLink.newWindowLink();
            };
            
            var $richtext = jQuery('#b2f39fc8-91a8-447a-a30c-d9ffbdd6062f.hiddenRte');
            if ($richtext) {
                CQ_Analytics.ClientContextUtils.onStoreInitialized('twcprofile', function() {
                    $richtext.each(function() {
                        if (window.Twc && Twc.Util && typeof Twc.Util.replaceClientContextVarsInElement === 'function') {
                            Twc.Util.replaceClientContextVarsInElement($(this));
                            var linkValue = $(this).find("a").data('href');
                            $(this).find('a').attr('href', linkValue);
                        }
                        $(this).removeClass('hiddenRte hidden');
                    })
                }, 0);
            }
        });
    }
    </script>


