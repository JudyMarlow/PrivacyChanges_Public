<script type="text/javascript">
	STAPLES.Onload.addWindowEvent(function(){
		function loadjscssfile(filename, filetype){
	 		if (filetype=="js"){
	  			var fileref=document.createElement('script');
	  			fileref.setAttribute("type","text/javascript");
	  			fileref.setAttribute("src", filename);
			}
	 		else if (filetype=="css"){
	  			var fileref=document.createElement("link");
	  			fileref.setAttribute("rel", "stylesheet");
	  			fileref.setAttribute("type", "text/css");
	  			fileref.setAttribute("href", filename);
	 		}
	 		if (typeof fileref!="undefined")
  				document.getElementsByTagName("head")[0].appendChild(fileref);
	 	}
	
		loadjscssfile("/sbdpas/20150826065325/onlineopinionV5/oo_style_english.css", "css"); //MAIN OPINIONLAB STYLESHEET
		 
			var pageName = document.getElementById("orcPageName");	
			 if(typeof(pageName) != 'undefined' && pageName != null && (pageName.value === 'returnItems' || pageName.value === 'returnDetails' || pageName.value === 'verifyAddress')){
			 	loadjscssfile("/sbdpas/20150826065325/onlineopinionV5/oo_conf_exit_staples.js", "js"); //MAIN OPINIONLAB ENGINE 
			 } else {
			 	loadjscssfile("/sbdpas/20150826065325/onlineopinionV5/oo_tab.js", "js"); //MAIN OPINIONLAB ENGINE 
			 }
					
	});
</script>

<script type="text/javascript">
STAPLES.Onload.addWindowEvent(function(){
    function loadPowerFront(){
        var inside = document.createElement('script'); inside.type = 'text/javascript'; inside.async = true;
        inside.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stp-tracker.inside-graph.com/gtm/IN-1000306/include.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(inside, s);
    }
    loadPowerFront();
});
</script>
<!-- end footer --></div> <script type="text/javascript">
	/* ************ DO NOT ALTER ANYTHING BELOW THIS LINE ! ************* */
	// store all data in the s object into a temp variable so it doesn't get overridden when s_code.js loads.
	var sc_temp = s;
	
	// add the tracking tag to the page
	function s_omniPageLoadTrack() {
		// pass s code data stored in temp to s_code.js
		for (var i in sc_temp){
			s[i] = sc_temp[i];
		}
		// add tracking tag to page
		try {
			var s_code=s.t();
			if(s_code)$("body").append(s_code);
		} catch(err){
		
		}
	}
	// check if scode is supposed be loaded
	if(propertyValues.scodeSwitch == 'ON'){
		// load the scode.js file asynchronously
		$.getScript('/sbdpas/js/omniture/s_code.js',function(){ 
			// After successful load of s_code and DOM is ready
			$(function(){
				s_omniPageLoadTrack();
			});
		});
	}

